        "        llm,\n",
        "        df_completo,\n",
        "        prefix=AGENT_PREFIX,\n",
        "        verbose=False,\n",
        "        allow_dangerous_code=True,\n",
        "        # Habilitando o modo de autocorreção da maneira correta:\n",
        "        agent_executor_kwargs={'handle_parsing_errors': True},\n",
        "    )\n",
        "    print(\"✅ Agente final, com autocorreção, criado com sucesso!\")\n",
        "\n",
        "else:\n",
